{
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "es2016",
            "dom"
        ],
        "module": "commonjs",
        "rootDir": "./",
        "moduleResolution": "node",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}